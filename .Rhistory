dat <- slice(dat,i)
tmp <-  expand.grid(Row=1:n, Column=1:n) %>%
mutate(id=i, label=dat$label,
value = unlist(dat[,-1]))
tmp%>%ggplot(aes(Row, Column, fill=value)) +
geom_raster() +
scale_y_reverse() +
scale_fill_gradient(low="white", high="black") +
ggtitle(tmp$label[1])
}
